				<key>script</key>
				<string>query="{query}"

PATH=/usr/local/bin:$PATH
if command -v dark-mode &amp;&gt; /dev/null; then
	args=''
	case "$query" in
	"" | on | ON)
		args='on'
		;;
	off | OFF)
		args='off'
		;;
	switch|SWITCH)
		args=''
		;;
	*)
		args='on'
		;;
	esac
	dark-mode $args
else
	echo -n 'e.g. brew install dark-mode'
fi</string>
